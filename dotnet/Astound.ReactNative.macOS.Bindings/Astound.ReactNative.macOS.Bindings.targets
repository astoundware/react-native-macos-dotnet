
<Project>
    <ItemGroup Condition="'$(Configuration)'=='Release'">
        <Reference Include="Astound.ReactNative.macOS.Bindings">
            <HintPath>$(MSBuildThisFileDirectory)..\reference\Release\Astound.ReactNative.macOS.Bindings.dll</HintPath>
        </Reference>
    </ItemGroup>
    <ItemGroup Condition="'$(Configuration)'!='Release'">
        <Reference Include="Astound.ReactNative.macOS.Bindings">
            <HintPath>$(MSBuildThisFileDirectory)..\reference\Debug\Astound.ReactNative.macOS.Bindings.dll</HintPath>
        </Reference>
    </ItemGroup>
    <ItemGroup>
        <BundleResource Include="$(MSBuildThisFileDirectory)..\content\AccessibilityResources.bundle\**\*.*">
          <Link>%(RecursiveDir)%(Filename)%(Extension)</Link>
        </BundleResource>
    </ItemGroup>
</Project>